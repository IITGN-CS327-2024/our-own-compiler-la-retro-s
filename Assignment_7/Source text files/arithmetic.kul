    int start(){
        int add(int x, int y){
            int z = x+y;
            return z;
        }
        int sub(int x, int y){
            int z = x-y;
            return z;
        }
        int mul(int x, int y){
            int z = x*y;
            return z;
        }
        int div(int x, int y){
            int z = x/y;
            return z;
        }
        int mod(int x, int y){
            int z = x%y;
            return z;
        }
        return 0;
    }